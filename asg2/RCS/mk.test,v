text
@#!/bin/sh
# $Id: mk.test,v 1.1 2016-09-30 16:02:16-07 - - $
for test in *.in
do
   echo $0: $test starting.
   base=`echo $test | sed 's/\.in$//'`
   dc <$test >$base.dcout 2>&1
   ocamldc <$test >$base.ocamldcout 2>&1
   diff $base.dcout $base.ocamldcout
   echo $0: $test finished.
done
@


